{
	"schemaVersion": 1,
	"id": "craftables",
	"version": "${version}",
	"name": "Uncraftables",
	"description": "Adds recipes to some normally uncraftable items.",
	"authors": [
		"Zakyy"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/craftables/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.zakyy.uncraftables.Uncraftables"
		],
		"client": [
			"net.zakyy.uncraftables.UncraftablesClient"
		],
		"fabric-datagen": [
			"net.zakyy.uncraftables.UncraftablesDataGenerator"
		]
	},
	"mixins": [
		"craftables.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.22",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}